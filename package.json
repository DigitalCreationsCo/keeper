{
  "name": "keeper",
  "version": "1.2.7",
  "description": "Keeper is a developer tool that helps keep your documentation in sync with your code. It automatically generates documentation update tasks for an AI agent to complete after you commit your code.",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "test": "bash run_tests.sh",
    "install-hook": "bash install.sh",
    "release": "VERSION=\"v$(node -p \"require('./package.json').version\")\" bash release.sh"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DigitalCreationsCo/keeper.git"
  },
  "keywords": [
    "documentation",
    "git-hooks",
    "ai",
    "coding-agent",
    "developer-tools"
  ],
  "author": "Andres Bryant Mejia",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/DigitalCreationsCo/keeper/issues"
  },
  "homepage": "https://github.com/DigitalCreationsCo/keeper#readme"
}
